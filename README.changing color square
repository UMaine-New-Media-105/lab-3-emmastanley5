// Emma Stanley 
function setup() {
  createCanvas(400, 400);
}

function draw() {
  background("pink");
   
   if (mouseIsPressed) {
    fill("skyblue");
   } else if (mouseY > 100){
  fill('yellow') 
} else {
    fill("purple");
  }
  rect(50,100,200)
  ellipse(30,60,100)
  ellipse(295,300,100)
  
  
}
